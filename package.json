  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "cty",
  "homepage": "http://39.98.81.218/",
  "license": "MIT",
  "keywords": [
    "CloudBlock_Blank_Oracle",
    "antd",
    "ng-zorro-antd",
    "angular",
    "component",
    "scaffold"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "npm run lint-staged",
    "ng": "ng",
    "start": "ng serve -o",
    "serve:hmr": "ng serve -o --hmr -e=hmr",
    "build": "npm run color-less && node --max_old_space_size=8196 ./node_modules/@angular/cli/bin/ng build  --prod --build-optimizer",
    "build2": "npm run color-less && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build  --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "run-s lint:ts lint:style",
    "lint:ts": "ng lint",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "e2e": "ng e2e",
    "test-coverage": "ng test -sr -cc -sm=false",
    "debug-apk": "cordova build android --debug",
    "color-less": "node scripts/color-less.js",
    "gc": "cd src/app/routes/pages && ng g c",
    "gclass": "cd src/app/routes/pages && ng g class",
    "serve": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve --port 4202"
  },
